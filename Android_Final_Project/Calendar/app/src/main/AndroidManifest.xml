<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.hci.calendar">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".CalendarActivity"
            android:label="@string/app_name">


        </activity>
        <activity
            android:name=".MainActivity"
            android:label="NewEvent">

        </activity>
        <activity
            android:name=".Setting"
            android:label="SettingsMenu">

        </activity>
        <activity
            android:name=".NavigationActivity"
            android:label="NavigationMenu">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".StickyListviewActivity"
            android:label="DayView">

        </activity>

        <activity android:name=".AddToCalendar"
            android:label="AddToCalendar">
        </activity>


    </application>

</manifest>
